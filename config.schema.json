{
  "pluginAlias": "EufyRobovacHomebridgePlugin",
  "pluginType": "platform",
  "singular": true,
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "description": "Arbitrary name for your Eufy Robovac",
        "type": "string",
        "required": true,
        "default": "Eufy Robovac"
      },
      "ip": {
        "title": "IP Address",
        "description": "IP Address of your Eufy Robovac",
        "type": "string",
        "required": true,
        "default": "10.0.1.69"
      },
      "deviceId": {
        "title": "Tuya Device ID",
        "description": "Tuya/Eufy Device ID (see: https://github.com/markbajaj/eufy-device-id-python)",
        "type": "string",
        "required": true
      },
      "deviceKey": {
        "title": "Tuya Device Key",
        "description": "Tuya/Eufy Device/Local Key (see: https://github.com/markbajaj/eufy-device-id-python)",
        "type": "string",
        "required": true
      }
    }
  }
}
